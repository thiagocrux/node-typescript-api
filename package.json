{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Typescript Node.js API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:functional": "jest --projects ./test --runInBand",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiagocrux/node-typescript-api.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Thiago Cruz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thiagocrux/node-typescript-api/issues"
  },
  "homepage": "https://github.com/thiagocrux/node-typescript-api#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.14.29",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "jest": "^26.6.3",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "module-alias": "^2.2.2"
  }
}
